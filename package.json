{
  "name": "begacy-backend",
  "version": "0.0.1",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "begacy api server",
  "private": true,
  "scripts": {
    "ts": "node -r esbuild-register -r dotenv/config",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "nodemon",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "yarn ts --transpile-only src/schema",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "studio": "npx prisma studio --experimental",
    "agendash": "agendash --db=mongodb://mongodb/agenda",
    "build": "npm -s run clean && etsc",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "generate-types": "typechain --target=ethers-v5 'abis/*.json' --out-dir=src/types/ethers",
    "compile": "yarn generate-types"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
